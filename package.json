{
  "name": "@pocket/web-client",
  "version": "1.0.0",
  "description": "Pocket front end client",
  "main": "index.js",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "scripts": {
    "dev": "RELEASE_VERSION=$(git describe --abbrev=0 --tags) SHOW_DEV=included node ./server.local.js",
    "build": "npm run locale-inject && next build",
    "start": "next start -p 80",
    "start-local": "AS_PRODUCTION=true node ./server.local.js",
    "cypress:launch": "CYPRESS_BASE_URL=http://localhost.web-client.getpocket.com cypress open",
    "cypress:run": "CYPRESS_BASE_URL=http://localhost.web-client.getpocket.com cypress run",
    "cypress:ci": "cypress run",
    "lint": "eslint src/** --quiet",
    "lint-diff": "./node_modules/.bin/eslint $(git diff main --name-only -- components/*.js connectors/*.js containers/*.js) --quiet",
    "test": "jest --reporters='default'",
    "test:ci": "jest --ci",
    "storybook": "start-storybook -p 9009 -s ./public -h localhost.web-client.getpocket.com",
    "build-storybook": "build-storybook",
    "locale-inject": "node ./.scripts/localization/locale-inject",
    "locale-parse": "node ./.scripts/localization/locale-parse"
  },
  "jest-junit": {
    "addFileAttribute": "true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pocket/web-client.git"
  },
  "author": "",
  "license": "ISC",
  "homepage": "https://github.com/Pocket/web-client#readme",
  "bugs": {
    "url": "https://github.com/Pocket/web-client/issues"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/git",
      [
        "@semantic-release/exec",
        {
          "prepareCmd": "echo ${nextRelease.version} > .version"
        }
      ]
    ]
  },
  "devDependencies": {
    "@babel/cli": "7.15.7",
    "@babel/core": "7.15.8",
    "@babel/eslint-parser": "7.15.8",
    "@babel/preset-env": "7.15.8",
    "@babel/preset-react": "7.14.5",
    "@commitlint/cli": "^13.2.0",
    "@commitlint/config-conventional": "^13.2.0",
    "@semantic-release/changelog": "6.0.0",
    "@semantic-release/commit-analyzer": "9.0.1",
    "@semantic-release/exec": "6.0.1",
    "@semantic-release/git": "10.0.0",
    "@semantic-release/release-notes-generator": "10.0.2",
    "@storybook/addon-actions": "6.3.5",
    "@storybook/addon-essentials": "6.3.5",
    "@storybook/addon-links": "6.3.5",
    "@storybook/react": "6.3.5",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.0.0",
    "@testing-library/user-event": "13.2.1",
    "babel-loader": "8.2.2",
    "babel-plugin-module-resolver": "4.1.0",
    "chalk": "4.1.2",
    "enquirer": "2.3.6",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-cypress": "2.11.3",
    "eslint-plugin-flowtype": "5.9.2",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jest-dom": "3.9.0",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.25.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-testing-library": "4.12.0",
    "file-loader": "6.2.0",
    "fs-extra": "10.0.0",
    "graphql": "15.5.2",
    "gulp-sort": "2.0.0",
    "i18next-parser": "4.4.0",
    "isomorphic-unfetch": "3.1.0",
    "jest": "27.1.0",
    "jest-junit": "12.2.0",
    "jsdom": "17.0.0",
    "jsdom-global": "3.0.2",
    "next-build-id": "3.0.0",
    "prettier": "2.3.2",
    "simple-git": "2.45.0",
    "vinyl-fs": "3.0.3"
  },
  "dependencies": {
    "@n8tb1t/use-scroll-position": "2.0.3",
    "@pocket/web-localization": "0.16.0",
    "@pocket/web-ui": "2.36.3",
    "@pocket/web-utilities": "1.16.7",
    "@sentry/integrations": "6.12.0",
    "@sentry/nextjs": "6.12.0",
    "classnames": "2.3.1",
    "clipboard-copy": "4.0.1",
    "dayjs": "1.10.6",
    "diff-match-patch": "1.0.5",
    "dompurify": "2.3.1",
    "graphql-request": "3.5.0",
    "husky": "7.0.2",
    "intersection-observer": "0.12.0",
    "linaria": "2.1.0",
    "match-sorter": "6.3.0",
    "mousetrap": "1.6.5",
    "next": "11.1.2",
    "next-i18next": "8.6.0",
    "next-linaria": "0.11.0",
    "next-redux-saga": "4.1.2",
    "next-redux-wrapper": "7.0.2",
    "node-fetch": "2.6.1",
    "nookies": "2.5.2",
    "prop-types": "15.7.2",
    "query-string": "7.0.1",
    "raf": "3.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-ga": "3.3.0",
    "react-google-recaptcha": "2.1.0",
    "react-i18next": "11.11.4",
    "react-input-autosize": "3.0.0",
    "react-intersection-observer": "8.32.1",
    "react-markdown": "6.0.2",
    "react-redux": "7.2.4",
    "react-share": "4.4.0",
    "react-visibility-sensor": "5.1.1",
    "redux": "4.1.1",
    "redux-saga": "1.1.3",
    "storage-factory": "0.2.0",
    "validator": "13.6.0"
  }
}
