{
  "name": "@client/web",
  "version": "1.0.0",
  "description": "Pocket front end web client",
  "main": "index.js",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9"
  },
  "scripts": {
    "dev": "RELEASE_VERSION=$(git describe --abbrev=0 --tags) SHOW_DEV=included node ./server.local.js",
    "dev:snowplow": "TEST_SNOWPLOW=included pnpm dev",
    "analyze": "ANALYZE=true next build",
    "build": "next build",
    "start": "next start -p 80",
    "start:local": "AS_PRODUCTION=true node ./server.local.js",
    "lint": "next lint",
    "test": "jest --reporters='default'",
    "test:ci": "jest --ci",
    "test:update": "jest --u",
    "clean": "rm -rf .turbo && rm -rf node_modules",
    "locale:inject": "node ./common/localization/locale-inject.mjs",
    "locale:parse": "cd common/localization/ && pnpm extract",
    "schema": "rover graph fetch pocket-client-api@current >| schema.graphql",
    "checktypes": "tsc --noEmit --incremental false"
  },
  "jest-junit": {
    "addFileAttribute": "true"
  },
  "license": "ISC",
  "devDependencies": {
    "@apollo/rover": "0.23.0",
    "@common/localization": "workspace:*",
    "@common/types": "workspace:*",
    "@config/eslint-config": "workspace:*",
    "@config/jest": "workspace:^",
    "@config/typescript-config": "workspace:*",
    "@graphql-eslint/eslint-plugin": "3.20.1",
    "@next/eslint-plugin-next": "14.2.4",
    "@testing-library/jest-dom": "6.4.6",
    "@testing-library/react": "16.0.0",
    "@testing-library/user-event": "14.5.2",
    "@types/node": "20.14.2",
    "@types/react": "18.3.3",
    "@types/react-beautiful-dnd": "13.1.8",
    "eslint": "8.57.0",
    "graphql": "16.8.2",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-junit": "16.0.0",
    "next-router-mock": "0.9.13",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-nested": "^6.0.1",
    "postcss-preset-env": "^9.5.14",
    "prettier": "3.3.2",
    "typescript": "5.4.5"
  },
  "dependencies": {
    "@braze/web-sdk": "5.3.1",
    "@emotion/cache": "11.11.0",
    "@emotion/css": "11.11.2",
    "@emotion/react": "11.11.4",
    "@freestar/pubfig-adslot-react-component": "3.6.1",
    "@n8tb1t/use-scroll-position": "2.0.3",
    "@popperjs/core": "2.11.8",
    "@sentry/integrations": "7.114.0",
    "@sentry/nextjs": "7.114.0",
    "@storybook/react": "8.1.10",
    "@types/jest": "^29.5.12",
    "@ui/icons": "workspace:*",
    "@ui/styles": "workspace:*",
    "body-scroll-lock": "4.0.0-beta.0",
    "clipboard-copy": "4.0.1",
    "dayjs": "1.11.11",
    "diff-match-patch": "1.0.5",
    "dompurify": "3.1.5",
    "he": "1.2.0",
    "intersection-observer": "0.12.2",
    "jsonwebtoken": "9.0.2",
    "legal-docs": "github:mozilla/legal-docs#7c2fffcbb56f16fa88eb7eed9d52650c5fcd95b1",
    "marked": "13.0.0",
    "match-sorter": "6.3.4",
    "mousetrap": "1.6.5",
    "next": "14.2.4",
