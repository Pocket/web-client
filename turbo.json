{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": [
    "ASSET_PREFIX",
    "NODE_ENV",
    "TEST_SNOWPLOW",
    "SHOW_DEV",
    "RELEASE_VERSION",
    "BRAZE_LOGS",
    "BRAZE_PRIVATE_KEY",
    "REVALIDATION_TOKEN"
  ],
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        "dist/**"
      ]
    },
    "test": {},
    "test:update": {},
    "lint": {},
    "checktypes": {},
    "dev": {
      "cache": false,
      "persistent": true
    },
    "dev:snowplow": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    },
    "storybook": {
      "dependsOn": [
        "^build"
      ],
      "cache": false,
      "persistent": true
    }
  }
}
