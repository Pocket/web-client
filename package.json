{
  "name": "@pocket/web-client",
  "version": "1.0.0",
  "description": "Pocket front end client",
  "main": "index.js",
  "scripts": {
    "dev": "SHOW_DEV=included next dev -p 80",
    "build": "npm run locale-inject && SHOW_DEV=included next build",
    "start": "next start -p 80",
    "lint": "./node_modules/.bin/eslint $(find src/ -name '*.js' ! -name '*.stories.js' ! -name '*.spec.js')",
    "lint-diff": "./node_modules/.bin/eslint $(git diff master --name-only -- components/*.js connectors/*.js containers/*.js) --quiet",
    "test": "NODE_PATH=./src:./public:$NODE_PATH NODE_ENV=test mocha --reporter spec --harmony --require .scripts/babel-register .scripts/test-setup.js $(find src/ -name '*.spec.js')",
    "storybook": "start-storybook -s ./public -h localhost.web.getpocket.com",
    "build-storybook": "build-storybook",
    "locale-inject": "node ./.scripts/localization/locale-inject",
    "locale-parse": "node ./.scripts/localization/locale-parse"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pocket/web-client.git"
  },
  "author": "",
  "license": "ISC",
  "homepage": "https://github.com/Pocket/web-client#readme",
  "bugs": {
    "url": "https://github.com/Pocket/web-client/issues"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@babel/cli": "7.12.8",
