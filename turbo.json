{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": [
    "ASSET_PREFIX",
    "NODE_ENV",
    "TEST_SNOWPLOW",
    "SHOW_DEV",
    "RELEASE_VERSION",
    "BRAZE_LOGS",
    "BRAZE_PRIVATE_KEY",
    "REVALIDATION_TOKEN"
  ],
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "test": {
      "dependsOn": ["^build"]
    },
    "test:ci": {
      "dependsOn": ["^build"]
    },
    "test:update": {},
    "lint": {},
    "checktypes": {},
    "pocket": {
      "cache": false,
      "persistent": true
    },
    "dev": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": true
    },
    "dev:snowplow": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    },
    "schema": {
      "cache": false
    },
    "scraper": {
      "persistent": true
    },
    "storybook": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": true
    },
    "synth": {
      "inputs": ["src/**/*", "package.json", "cdktf.json", ".terraform-version"],
      "outputs": ["cdktf.out/**/*"]
    }
  }
}
