{
  "name": "@pocket/web-client",
  "version": "1.0.0",
  "description": "Pocket front end client",
  "main": "index.js",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "prep": "npm run locale:inject && npm run compile:icons",
    "dev": "npm run prep && RELEASE_VERSION=$(git describe --abbrev=0 --tags) SHOW_DEV=included node ./server.local.js",
    "analyze": "npm run prep && ANALYZE=true next build",
    "build": "npm run prep && next build",
    "start": "next start -p 80",
    "start:local": "AS_PRODUCTION=true node ./server.local.js",
    "lint": "next lint",
    "test": "npm run compile:icons && jest --reporters='default'",
    "test:e2e": "node ./.scripts/qa/e2e.js",
    "test:ci": "npm run compile:icons && jest --ci",
    "test:snowplow": "RELEASE_VERSION=$(git describe --abbrev=0 --tags) SHOW_DEV=included TEST_SNOWPLOW=included node ./server.local.js",
    "storybook": "npm run prep && cd .scripts/storybook && npm install && npm run storybook",
    "storybook:build": "build-storybook",
    "compile": "npm run compile:icons & npm run compile:styles",
    "compile:icons": "node ./.scripts/icons/compile-icons.js",
    "compile:styles": "node .scripts/styles/compile-styles.js",
    "locale:inject": "node ./.scripts/localization/locale-inject",
    "locale:parse": "node ./.scripts/localization/locale-parse",
    "schema": "rover graph fetch pocket-client-api@current >| schema.graphql",
    "prettier:check": "prettier --check 'src/**/*.{js,jsx,ts,tsx}'",
    "postinstall": "cd .scripts/localization && npm install -s && cd ../icons && npm install -s && cd ../styles && npm install -s"
  },
  "jest-junit": {
    "addFileAttribute": "true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pocket/web-client.git"
  },
  "author": "",
  "license": "ISC",
  "homepage": "https://github.com/Pocket/web-client#readme",
  "bugs": {
    "url": "https://github.com/Pocket/web-client/issues"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/git",
      [
        "@semantic-release/exec",
        {
          "prepareCmd": "echo ${nextRelease.version} > .version"
        }
      ]
    ]
  },
  "devDependencies": {
    "@apollo/rover": "0.14.0",
    "@babel/cli": "7.21.5",
    "@babel/core": "7.21.8",
    "@babel/eslint-parser": "7.21.8",
    "@babel/preset-env": "7.21.5",
    "@babel/preset-react": "7.18.6",
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@graphql-eslint/eslint-plugin": "3.18.0",
    "@next/eslint-plugin-next": "13.4.1",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "9.0.2",
    "@semantic-release/exec": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/release-notes-generator": "11.0.1",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/node": "20.0.0",
    "@types/react": "18.2.5",
    "@typescript-eslint/eslint-plugin": "5.59.2",
    "babel-loader": "9.1.2",
    "babel-plugin-module-resolver": "5.0.0",
    "eslint": "8.39.0",
    "eslint-config-next": "13.4.1",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-cypress": "2.13.3",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "graphql": "16.6.0",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "jest-junit": "16.0.0",
    "jsdom": "22.0.0",
    "jsdom-global": "3.0.2",
    "next-build-id": "3.0.0",
    "next-router-mock": "0.9.3",
    "prettier": "2.8.8",
    "react-swipeable": "7.0.0",
    "typescript": "5.0.4"
  },
  "dependencies": {
    "@braze/web-sdk": "4.7.0",
    "@freestar/pubfig-adslot-react-component": "3.6.0",
    "@n8tb1t/use-scroll-position": "2.0.3",
    "@next/bundle-analyzer": "13.4.1",
    "@pocket/web-localization": "0.27.0",
    "@popperjs/core": "2.11.7",
    "@sentry/integrations": "7.51.0",
    "@sentry/nextjs": "7.51.0",
    "body-scroll-lock": "4.0.0-beta.0",
    "clipboard-copy": "4.0.1",
    "dayjs": "1.11.7",
    "diff-match-patch": "1.0.5",
    "dompurify": "3.0.2",
    "intersection-observer": "0.12.2",
    "jsonwebtoken": "9.0.0",
    "linaria": "2.3.1",
    "match-sorter": "6.3.1",
    "mousetrap": "1.6.5",
    "next": "13.4.1",
    "next-i18next": "13.2.2",
    "next-linaria": "0.11.0",
    "next-redux-saga": "4.1.2",
    "next-redux-wrapper": "8.1.0",
    "node-fetch": "3.3.1",
    "nookies": "2.5.2",
    "prop-types": "15.8.1",
    "query-string": "7.1.1",
    "react": "18.2.0",
    "react-beautiful-dnd": "13.1.1",
    "react-dom": "18.2.0",
    "react-google-recaptcha": "2.1.0",
    "react-i18next": "12.2.2",
    "react-intersection-observer": "9.4.3",
    "react-joyride": "2.5.4",
    "react-markdown": "8.0.7",
    "react-modal": "3.16.1",
    "react-popper": "2.3.0",
    "react-redux": "8.0.5",
    "react-textarea-autosize": "8.4.1",
    "react-visibility-sensor": "5.1.1",
    "redux": "4.2.1",
    "redux-saga": "1.2.3",
    "storage-factory": "0.2.1",
    "validator": "13.9.0"
  }
}
