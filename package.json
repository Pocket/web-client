{
  "name": "@pocket/web-client",
  "version": "1.0.0",
  "description": "Pocket front end client",
  "main": "index.js",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.3",
  "scripts": {
    "prep": "pnpm compile:icons",
    "dev": "pnpm prep && RELEASE_VERSION=$(git describe --abbrev=0 --tags) SHOW_DEV=included node ./server.local.js",
    "analyze": "pnpm prep && ANALYZE=true next build",
    "build": "pnpm prep && next build",
    "start": "next start -p 80",
    "start:local": "AS_PRODUCTION=true node ./server.local.js",
    "lint": "next lint",
    "test": "pnpm compile:icons && jest --reporters='default'",
    "test:e2e": "node ./.scripts/qa/e2e.js",
    "test:ci": "pnpm compile:icons && jest --ci",
    "test:snowplow": "RELEASE_VERSION=$(git describe --abbrev=0 --tags) SHOW_DEV=included TEST_SNOWPLOW=included node ./server.local.js",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:start": "pnpm dlx serve ./storybook-static",
    "compile": "pnpm compile:icons & pnpm compile:styles",
    "compile:icons": "node ./.scripts/icons/compile-icons.js",
    "compile:styles": "node .scripts/styles/compile-styles.js",
    "locale:inject": "node ./.scripts/localization/locale-inject.mjs",
    "locale:parse": "cd .scripts/localization/ && pnpm extract",
    "schema": "rover graph fetch pocket-client-api@current >| schema.graphql",
    "prettier:check": "prettier --check 'src/**/*.{js,jsx,ts,tsx}'",
    "postinstall": "cd .scripts/localization && pnpm i -s && cd ../icons && pnpm i -s && cd ../styles && pnpm i -s"
  },
  "jest-junit": {
    "addFileAttribute": "true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pocket/web-client.git"
  },
  "author": "",
  "license": "ISC",
  "homepage": "https://github.com/Pocket/web-client#readme",
  "bugs": {
    "url": "https://github.com/Pocket/web-client/issues"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/git",
      [
        "@semantic-release/exec",
        {
          "prepareCmd": "echo ${nextRelease.version} > .version"
        }
      ]
    ]
  },
  "devDependencies": {
    "@apollo/rover": "0.22.0",
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.2",
    "@graphql-eslint/eslint-plugin": "3.20.1",
    "@next/eslint-plugin-next": "14.1.0",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "11.1.0",
    "@semantic-release/exec": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/release-notes-generator": "12.1.0",
    "@storybook/addon-essentials": "7.6.17",
    "@storybook/addon-interactions": "7.6.17",
    "@storybook/addon-links": "7.6.17",
    "@storybook/blocks": "7.6.17",
    "@storybook/nextjs": "7.6.17",
    "@storybook/react": "7.6.17",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/jest-dom": "6.4.2",
    "@testing-library/react": "14.2.1",
    "@testing-library/user-event": "14.5.2",
    "@types/node": "20.11.20",
    "@types/react": "18.2.59",
    "@typescript-eslint/eslint-plugin": "7.1.0",
    "eslint": "8.57.0",
    "eslint-config-next": "14.1.0",
    "eslint-config-prettier": "9.1.0",
    "graphql": "16.8.1",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-junit": "16.0.0",
    "next-router-mock": "0.9.12",
    "prettier": "3.2.5",
    "react-swipeable": "7.0.1",
    "storybook": "7.6.17",
    "typescript": "5.3.3"
  },
  "dependencies": {
    "@braze/web-sdk": "5.1.1",
    "@emotion/css": "11.11.2",
    "@emotion/react": "11.11.3",
    "@freestar/pubfig-adslot-react-component": "3.6.0",
    "@n8tb1t/use-scroll-position": "2.0.3",
    "@popperjs/core": "2.11.8",
    "@sentry/integrations": "7.102.1",
    "@sentry/nextjs": "7.102.1",
    "body-scroll-lock": "4.0.0-beta.0",
    "clipboard-copy": "4.0.1",
    "dayjs": "1.11.10",
    "diff-match-patch": "1.0.5",
    "dompurify": "3.0.9",
    "intersection-observer": "0.12.2",
    "jsonwebtoken": "9.0.2",
    "match-sorter": "6.3.4",
    "mousetrap": "1.6.5",
    "next": "14.1.0",
    "next-i18next": "15.2.0",
    "next-redux-saga": "4.1.2",
    "next-redux-wrapper": "8.1.0",
    "node-fetch": "3.3.2",
    "nookies": "2.5.2",
    "prop-types": "15.8.1",
    "query-string": "7.1.1",
    "react": "18.2.0",
    "react-beautiful-dnd": "13.1.1",
    "react-dom": "18.2.0",
    "react-google-recaptcha": "3.1.0",
    "react-joyride": "2.7.4",
    "react-markdown": "9.0.1",
    "react-modal": "3.16.1",
    "react-popper": "2.3.0",
    "react-redux": "9.1.0",
    "react-textarea-autosize": "8.5.3",
    "react-visibility-sensor": "5.1.1",
    "redux": "5.0.1",
    "redux-saga": "1.3.0",
    "storage-factory": "0.2.1",
    "validator": "13.11.0"
  },
  "pnpm": {
    "overrides": {
      "axios@>=0.8.1 <0.28.0": ">=0.28.0"
    }
  }
}
