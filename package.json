{
  "name": "@pocket/web-client",
  "version": "1.0.0",
  "description": "Pocket front end client",
  "main": "index.js",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "RELEASE_VERSION=$(git describe --abbrev=0 --tags) SHOW_DEV=included node ./server.local.js",
    "build": "npm run locale:inject && next build",
    "start": "next start -p 80",
    "start:local": "AS_PRODUCTION=true node ./server.local.js",
    "cypress:launch": "CYPRESS_BASE_URL=http://localhost.web-client.getpocket.com cypress open",
    "cypress:run": "CYPRESS_BASE_URL=http://localhost.web-client.getpocket.com cypress run",
    "cypress:ci": "cypress run",
    "lint": "next lint",
    "test": "jest --reporters='default'",
    "test:ci": "jest --ci",
    "test:snowplow": "RELEASE_VERSION=$(git describe --abbrev=0 --tags) SHOW_DEV=included TEST_SNOWPLOW=included node ./server.local.js",
    "storybook": "start-storybook -p 9009 -s ./public -h localhost.web-client.getpocket.com",
    "storybook:build": "build-storybook",
    "locale:inject": "node ./.scripts/localization/locale-inject",
    "locale:parse": "node ./.scripts/localization/locale-parse",
    "schema": "rover graph fetch pocket-client-api@current >| schema.graphql"
  },
  "jest-junit": {
    "addFileAttribute": "true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pocket/web-client.git"
  },
  "author": "",
  "license": "ISC",
  "homepage": "https://github.com/Pocket/web-client#readme",
  "bugs": {
    "url": "https://github.com/Pocket/web-client/issues"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/git",
      [
        "@semantic-release/exec",
        {
          "prepareCmd": "echo ${nextRelease.version} > .version"
        }
      ]
    ]
  },
  "devDependencies": {
    "@apollo/rover": "0.6.0",
    "@babel/cli": "7.17.10",
    "@babel/core": "7.17.12",
    "@babel/eslint-parser": "7.17.0",
    "@babel/preset-env": "7.17.12",
    "@babel/preset-react": "7.17.12",
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "@graphql-eslint/eslint-plugin": "3.10.3",
    "@next/eslint-plugin-next": "12.1.6",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/commit-analyzer": "9.0.2",
    "@semantic-release/exec": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/release-notes-generator": "10.0.3",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.2.0",
    "@testing-library/user-event": "14.2.0",
    "babel-loader": "8.2.5",
    "babel-plugin-module-resolver": "4.1.0",
    "eslint": "8.15.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.0.0",
    "graphql": "16.5.0",
    "husky": "8.0.1",
    "jest": "28.1.0",
    "jest-environment-jsdom": "28.1.0",
    "jest-junit": "13.2.0",
    "jsdom": "19.0.0",
    "jsdom-global": "3.0.2",
    "next-build-id": "3.0.0",
    "prettier": "2.6.2"
  },
  "dependencies": {
    "@braze/web-sdk": "4.0.1",
    "@n8tb1t/use-scroll-position": "2.0.3",
    "@pocket/web-localization": "0.20.0",
    "@pocket/web-ui": "2.36.5",
    "@sentry/integrations": "6.19.7",
    "@sentry/nextjs": "6.19.7",
    "classnames": "2.3.1",
    "clipboard-copy": "4.0.1",
    "dayjs": "1.11.2",
    "diff-match-patch": "1.0.5",
    "dompurify": "2.3.8",
    "graphql-request": "4.2.0",
    "intersection-observer": "0.12.0",
    "linaria": "2.3.1",
    "match-sorter": "6.3.1",
    "mousetrap": "1.6.5",
    "next": "12.1.6",
    "next-i18next": "11.0.0",
    "next-linaria": "0.11.0",
    "next-redux-saga": "4.1.2",
    "next-redux-wrapper": "7.0.5",
    "node-fetch": "3.2.4",
    "nookies": "2.5.2",
    "prop-types": "15.8.1",
    "q-flat": "1.0.7",
    "query-string": "7.1.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-google-recaptcha": "2.1.0",
    "react-i18next": "11.16.9",
    "react-intersection-observer": "9.1.0",
    "react-markdown": "8.0.3",
    "react-modal": "3.15.1",
    "react-redux": "8.0.1",
    "react-visibility-sensor": "5.1.1",
    "redux": "4.2.0",
    "redux-saga": "1.1.3",
    "storage-factory": "0.2.0",
    "validator": "13.7.0"
  }
}
