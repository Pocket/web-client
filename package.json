{
  "name": "@pocket/web-client",
  "version": "1.0.0",
  "description": "Pocket front end client",
  "main": "index.js",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "prep": "npm run locale:inject && npm run compile:icons",
    "dev": "npm run prep && RELEASE_VERSION=$(git describe --abbrev=0 --tags) SHOW_DEV=included node ./server.local.js",
    "analyze": "npm run prep && ANALYZE=true next build",
    "build": "npm run prep && next build",
    "start": "next start -p 80",
    "start:local": "AS_PRODUCTION=true node ./server.local.js",
    "cypress:launch": "CYPRESS_BASE_URL=http://localhost.web-client.getpocket.com cypress open",
    "cypress:run": "CYPRESS_BASE_URL=http://localhost.web-client.getpocket.com cypress run",
    "cypress:ci": "cypress run",
    "lint": "next lint",
    "test": "npm run compile:icons && jest --reporters='default'",
    "test:ci": "npm run compile:icons && jest --ci",
    "test:snowplow": "RELEASE_VERSION=$(git describe --abbrev=0 --tags) SHOW_DEV=included TEST_SNOWPLOW=included node ./server.local.js",
    "storybook": "cd .scripts/storybook && npm install && npm run storybook",
    "storybook:build": "build-storybook",
    "compile": "npm run compile:icons & npm run compile:styles",
    "compile:icons": "node ./.scripts/icons/compile-icons.js",
    "compile:styles": "node .scripts/styles/compile-styles.js",
    "locale:inject": "node ./.scripts/localization/locale-inject",
    "locale:parse": "node ./.scripts/localization/locale-parse",
    "schema": "rover graph fetch pocket-client-api@current >| schema.graphql",
    "postinstall": "cd .scripts/localization && npm install -s && cd ../icons && npm install -s && cd ../styles && npm install -s"
  },
  "jest-junit": {
    "addFileAttribute": "true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pocket/web-client.git"
  },
  "author": "",
  "license": "ISC",
  "homepage": "https://github.com/Pocket/web-client#readme",
  "bugs": {
    "url": "https://github.com/Pocket/web-client/issues"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/git",
      [
        "@semantic-release/exec",
        {
          "prepareCmd": "echo ${nextRelease.version} > .version"
        }
      ]
    ]
  },
  "devDependencies": {
    "@apollo/rover": "0.6.0",
    "@babel/cli": "7.17.10",
    "@babel/core": "7.17.12",
    "@babel/eslint-parser": "7.17.0",
    "@babel/preset-env": "7.17.12",
    "@babel/preset-react": "7.17.12",
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "@graphql-eslint/eslint-plugin": "3.10.3",
    "@next/eslint-plugin-next": "12.1.6",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/commit-analyzer": "9.0.2",
    "@semantic-release/exec": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/release-notes-generator": "10.0.3",
    "@testing-library/jest-dom": "5.16.2",
    "@testing-library/react": "12.1.4",
    "@testing-library/user-event": "13.5.0",
    "babel-loader": "8.2.5",
    "babel-plugin-module-resolver": "4.1.0",
    "eslint": "8.15.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.0.0",
    "graphql": "16.5.0",
    "husky": "8.0.1",
    "jest": "28.1.0",
    "jest-environment-jsdom": "28.1.0",
    "jest-junit": "13.2.0",
    "jsdom": "19.0.0",
    "jsdom-global": "3.0.2",
    "next-build-id": "3.0.0",
    "prettier": "2.6.2"
  },
  "dependencies": {
    "@braze/web-sdk": "4.0.1",
    "@n8tb1t/use-scroll-position": "2.0.3",
    "@next/bundle-analyzer": "12.1.6",
    "@pocket/web-localization": "0.22.0",
    "@popperjs/core": "2.11.5",
    "@sentry/integrations": "6.19.7",
    "@sentry/nextjs": "6.19.7",
    "body-scroll-lock": "4.0.0-beta.0",
    "clipboard-copy": "4.0.1",
    "dayjs": "1.11.2",
    "diff-match-patch": "1.0.5",
    "dompurify": "2.3.8",
    "graphql-request": "4.2.0",
    "intersection-observer": "0.12.0",
    "jsonwebtoken": "8.5.1",
    "linaria": "2.3.1",
    "match-sorter": "6.3.1",
    "mousetrap": "1.6.5",
    "next": "12.1.6",
    "next-i18next": "11.0.0",
    "next-linaria": "0.11.0",
    "next-redux-saga": "4.1.2",
    "next-redux-wrapper": "7.0.5",
    "node-fetch": "3.2.4",
    "nookies": "2.5.2",
    "prop-types": "15.8.1",
    "q-flat": "1.0.7",
    "query-string": "7.1.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-google-recaptcha": "2.1.0",
    "react-i18next": "11.16.9",
    "react-intersection-observer": "9.1.0",
    "react-markdown": "8.0.3",
    "react-modal": "3.15.1",
    "react-popper": "2.3.0",
    "react-redux": "8.0.1",
    "react-textarea-autosize": "8.3.4",
    "react-visibility-sensor": "5.1.1",
    "redux": "4.2.0",
    "redux-saga": "1.1.3",
    "storage-factory": "0.2.0",
    "validator": "13.7.0"
  }
}
