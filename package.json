{
  "name": "@pocket/web-client",
  "version": "1.0.0",
  "description": "Pocket front end client",
  "main": "index.js",
  "scripts": {
    "dev": "RELEASE_VERSION=$(git describe --abbrev=0 --tags) SHOW_DEV=included next dev -p 80",
    "build": "npm run locale-inject && next build",
    "start": "next start -p 80",
    "cypress:launch": "CYPRESS_BASE_URL=http://localhost.web-client.getpocket.com cypress open",
    "cypress:run": "CYPRESS_BASE_URL=http://localhost.web-client.getpocket.com cypress run",
    "cypress:ci": "cypress run",
    "lint": "./node_modules/.bin/eslint $(find src/ -name '*.js' ! -name '*.stories.js' ! -name '*.spec.js')",
    "lint-diff": "./node_modules/.bin/eslint $(git diff main --name-only -- components/*.js connectors/*.js containers/*.js) --quiet",
    "test": "npm run locale-inject && NODE_PATH=./src:./public:$NODE_PATH NODE_ENV=test mocha --reporter spec --harmony --require .scripts/babel-register .scripts/test-setup.js $(find src/ -name '*.spec.js')",
    "storybook": "start-storybook -p 9009 -s ./public -h localhost.web-client.getpocket.com",
    "build-storybook": "build-storybook",
    "locale-inject": "node ./.scripts/localization/locale-inject",
    "locale-parse": "node ./.scripts/localization/locale-parse"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pocket/web-client.git"
  },
  "author": "",
  "license": "ISC",
  "homepage": "https://github.com/Pocket/web-client#readme",
  "bugs": {
    "url": "https://github.com/Pocket/web-client/issues"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/git",
      [
        "@semantic-release/exec",
        {
          "prepareCmd": "echo ${nextRelease.version} > .version"
        }
      ]
    ]
  },
  "devDependencies": {
    "@babel/cli": "7.13.16",
    "@babel/core": "7.14.2",
    "@babel/eslint-parser": "7.14.2",
    "@babel/preset-env": "7.14.2",
    "@babel/preset-react": "7.13.13",
    "@babel/register": "7.13.16",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/exec": "5.0.0",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/release-notes-generator": "9.0.2",
    "@storybook/addon-essentials": "6.2.9",
    "@storybook/react": "6.2.9",
    "@zeit/next-source-maps": "0.0.3",
    "babel-loader": "8.2.2",
    "babel-plugin-module-resolver": "4.1.0",
    "chalk": "4.1.1",
    "cypress": "6.8.0",
    "enquirer": "2.3.6",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "eslint": "7.26.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-cypress": "2.11.3",
    "eslint-plugin-flowtype": "5.7.2",
    "eslint-plugin-import": "2.23.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.23.2",
    "eslint-plugin-react-hooks": "4.2.0",
    "extract-css-chunks-webpack-plugin": "4.9.0",
    "file-loader": "6.2.0",
    "fs-extra": "10.0.0",
    "graphql": "15.5.0",
    "gulp-sort": "2.0.0",
    "i18next-parser": "4.0.1",
    "isomorphic-unfetch": "3.1.0",
    "jsdom": "16.5.3",
    "jsdom-global": "3.0.2",
    "mocha": "8.4.0",
    "next-build-id": "3.0.0",
    "next-compose-plugins": "2.2.1",
    "optimize-css-assets-webpack-plugin": "5.0.5",
    "prettier": "2.3.0",
    "proxyquire": "2.1.3",
    "react-is": "17.0.2",
    "simple-git": "2.39.0",
    "sinon": "10.0.0",
    "storybook-addon-next-router": "2.0.4",
    "vinyl-fs": "3.0.3"
  },
  "dependencies": {
    "@n8tb1t/use-scroll-position": "2.0.3",
    "@pocket/web-localization": "0.9.2",
    "@pocket/web-ui": "2.35.2",
    "@pocket/web-utilities": "1.16.4",
    "@sentry/node": "6.4.0",
    "@sentry/react": "6.4.0",
    "@zeit/next-css": "1.0.1",
    "classnames": "2.3.1",
    "clipboard-copy": "4.0.1",
    "dayjs": "1.10.4",
    "diff-match-patch": "1.0.5",
    "dompurify": "2.2.8",
    "graphql-request": "3.4.0",
    "husky": "6.0.0",
    "intersection-observer": "0.12.0",
    "linaria": "2.1.0",
    "match-sorter": "6.3.0",
    "mousetrap": "1.6.5",
    "next": "^10.2.0",
    "next-i18next": "8.3.0",
    "next-redux-saga": "4.1.2",
    "next-redux-wrapper": "6.0.2",
    "node-fetch": "2.6.1",
    "nookies": "2.5.2",
    "prop-types": "15.7.2",
    "query-string": "7.0.0",
    "raf": "3.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-ga": "3.3.0",
    "react-google-recaptcha": "2.1.0",
    "react-i18next": "11.8.15",
    "react-input-autosize": "3.0.0",
    "react-intersection-observer": "8.32.0",
    "react-lorem-component": "0.13.0",
    "react-markdown": "6.0.2",
    "react-player": "2.9.0",
    "react-pose": "4.0.10",
    "react-redux": "7.2.4",
    "react-share": "4.4.0",
    "react-visibility-sensor": "5.1.1",
    "redux": "4.1.0",
    "redux-saga": "1.1.3",
    "storage-factory": "0.2.0",
    "validator": "13.6.0"
  }
}
